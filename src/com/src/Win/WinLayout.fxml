<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="821.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.src.Win.WinController">
   <children>
      <HBox alignment="CENTER" prefHeight="400.0" prefWidth="800.0" spacing="20.0">
         <children>
            <ListView fx:id="objectsView" prefHeight="400.0" prefWidth="338.0" />
            <VBox alignment="TOP_CENTER" prefHeight="400.0" prefWidth="245.0" spacing="30.0">
               <children>
                  <ComboBox fx:id="typeBox" prefWidth="150.0" />
                  <Button fx:id="characterButton" mnemonicParsing="false" text="Character" />
                  <Button fx:id="screenButton" mnemonicParsing="false" onAction="#handlePictureButton" text="Screen" />
                  <HBox prefHeight="38.0" prefWidth="245.0" spacing="20.0">
                     <children>
                        <Label text="Name:" />
                        <TextField fx:id="nameField" />
                     </children>
                  </HBox>
                  <GridPane fx:id="positionGrid" vgap="10.0">
                    <columnConstraints>
                      <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                      <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                    </columnConstraints>
                    <rowConstraints>
                      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                    </rowConstraints>
                     <children>
                        <TextField fx:id="startYField" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                        <Label text="StartX:" />
                        <TextField fx:id="startXField" GridPane.columnIndex="1" />
                        <Label text="EndX:" GridPane.rowIndex="1" />
                        <Label text="StartY:" GridPane.rowIndex="2" />
                        <Label text="EndY:" GridPane.rowIndex="3" />
                        <TextField fx:id="endXField" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                        <TextField fx:id="endYField" GridPane.columnIndex="1" GridPane.rowIndex="3" />
                     </children>
                  </GridPane>
               </children>
            </VBox>
         </children>
      </HBox>
   </children>
</AnchorPane>
